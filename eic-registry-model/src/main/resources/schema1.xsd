<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://einfracentral.eu" elementFormDefault="qualified" targetNamespace="http://einfracentral.eu" version="1.0">

  <xs:import schemaLocation="schema2.xsd"/>

  <xs:element name="bundle" type="tns:bundle"/>

  <xs:element name="catalogue" type="tns:catalogue"/>

  <xs:element name="catalogueBundle" type="tns:catalogueBundle"/>

  <xs:element name="dataSource" type="tns:dataSource"/>

  <xs:element name="dataSourceBundle" type="tns:dataSourceBundle"/>

  <xs:element name="emailMessage" type="tns:emailMessage"/>

  <xs:element name="event" type="tns:event"/>

  <xs:element name="helpdesk" type="tns:helpdesk"/>

  <xs:element name="identifier" type="tns:identifier"/>

  <xs:element name="infraService" type="tns:infraService"/>

  <xs:element name="loggingInfo" type="tns:loggingInfo"/>

  <xs:element name="metadata" type="tns:metadata"/>

  <xs:element name="migrationStatus" type="tns:migrationStatus"/>

  <xs:element name="monitoring" type="tns:monitoring"/>

  <xs:element name="monitoringGroup" type="tns:monitoringGroup"/>

  <xs:element name="multimediaPair" type="tns:multimediaPair"/>

  <xs:element name="provider" type="tns:provider"/>

  <xs:element name="providerBundle" type="tns:providerBundle"/>

  <xs:element name="providerLocation" type="tns:providerLocation"/>

  <xs:element name="providerMainContact" type="tns:providerMainContact"/>

  <xs:element name="providerMerilDomain" type="tns:providerMerilDomain"/>

  <xs:element name="providerPublicContact" type="tns:providerPublicContact"/>

  <xs:element name="providerRequest" type="tns:providerRequest"/>

  <xs:element name="rangeValue" type="tns:rangeValue"/>

  <xs:element name="service" type="tns:service"/>

  <xs:element name="serviceCategory" type="tns:serviceCategory"/>

  <xs:element name="serviceMainContact" type="tns:serviceMainContact"/>

  <xs:element name="serviceProviderDomain" type="tns:serviceProviderDomain"/>

  <xs:element name="servicePublicContact" type="tns:servicePublicContact"/>

  <xs:element name="useCasesPair" type="tns:useCasesPair"/>

  <xs:element name="user" type="tns:user"/>

  <xs:element name="vocabulary" type="tns:vocabulary"/>

  <xs:element name="vocabularyCuration" type="tns:vocabularyCuration"/>

  <xs:element name="vocabularyEntryRequest" type="tns:vocabularyEntryRequest"/>

  <xs:complexType abstract="true" name="bundle">
    <xs:sequence>
      <xs:element name="active" type="xs:boolean"/>
      <xs:element minOccurs="0" ref="tns:identifier"/>
      <xs:element minOccurs="0" name="latestAuditInfo" type="tns:loggingInfo"/>
      <xs:element minOccurs="0" name="latestOnboardingInfo" type="tns:loggingInfo"/>
      <xs:element minOccurs="0" name="latestUpdateInfo" type="tns:loggingInfo"/>
      <xs:element maxOccurs="unbounded" minOccurs="0" ref="tns:loggingInfo"/>
      <xs:element minOccurs="0" ref="tns:metadata"/>
      <xs:element minOccurs="0" ref="tns:migrationStatus"/>
      <xs:element name="suspended" type="xs:boolean"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="identifier">
    <xs:sequence>
      <xs:element minOccurs="0" name="alternativeIdentifiers">
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="alternativeIdentifier" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="hidden" type="xs:boolean"/>
      <xs:element minOccurs="0" name="originalId" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="loggingInfo">
    <xs:sequence>
      <xs:element default="null" minOccurs="0" name="actionType" type="xs:string"/>
      <xs:element default="null" minOccurs="0" name="comment" type="xs:string"/>
      <xs:element default="null" minOccurs="0" name="date" type="xs:string"/>
      <xs:element default="null" minOccurs="0" name="type" type="xs:string"/>
      <xs:element default="null" minOccurs="0" name="userEmail" type="xs:string"/>
      <xs:element default="null" minOccurs="0" name="userFullName" type="xs:string"/>
      <xs:element default="null" minOccurs="0" name="userRole" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="metadata">
    <xs:sequence>
      <xs:element default="null" minOccurs="0" name="modifiedAt" type="xs:string"/>
      <xs:element default="null" minOccurs="0" name="modifiedBy" type="xs:string"/>
      <xs:element default="null" minOccurs="0" name="originalId" type="xs:string"/>
      <xs:element default="null" minOccurs="0" name="registeredAt" type="xs:string"/>
      <xs:element default="null" minOccurs="0" name="registeredBy" type="xs:string"/>
      <xs:element default="null" minOccurs="0" name="source" type="xs:string"/>
      <xs:element minOccurs="0" name="terms">
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="term" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="migrationStatus">
    <xs:sequence>
      <xs:element minOccurs="0" name="comments">
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="comment" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element default="null" minOccurs="0" name="migrationDate" type="xs:string"/>
      <xs:element default="null" minOccurs="0" name="modelVersion" type="xs:string"/>
      <xs:element default="null" minOccurs="0" name="modified" type="xs:string"/>
      <xs:element default="null" minOccurs="0" name="resolutionDate" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="catalogue">
    <xs:sequence>
      <xs:element name="abbreviation" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[An abbreviation of the (Multi-Provider Regional or Thematic) Catalogue Name.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="affiliations">
        <xs:annotation>
          <xs:documentation><![CDATA[Catalogues that are members or affiliated or associated with other organisations should list those organisations here.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="affiliation" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="description" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[A high-level description of the Catalogue in fairly non-technical terms, with the vision, mission, objectives, background, experience.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="hostingLegalEntity" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Name of the organisation legally hosting (housing) the Catalogue or its coordinating centre.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="id" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[A persistent identifier, a unique reference to the  (Multi-Provider Regional or Thematic) Catalogue in the context of the EOSC Portal.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="legalEntity" type="xs:boolean">
        <xs:annotation>
          <xs:documentation><![CDATA[A Y/N question to define whether the (Multi-Provider Regional or Thematic) Catalogue is owned by a Legal Entity or not.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="legalStatus" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Legal status of the (Multi-Provider Regional or Thematic ) Catalogue Owner. The legal status is usually noted in the registration act/statutes.
For independent legal entities (1) - legal status of the Catalogue. For embedded Catalogues (2) - legal status of the hosting legal entity.
It is also possible to select Not a legal entity.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="location" type="tns:providerLocation">
        <xs:annotation>
          <xs:documentation><![CDATA[Physical location of the Catalogue.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="logo" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Link to the logo/visual identity of the Catalogue.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="mainContact" type="tns:providerMainContact">
        <xs:annotation>
          <xs:documentation><![CDATA[Catalogue's main contact info.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="multimedia">
        <xs:annotation>
          <xs:documentation><![CDATA[Link to video, slideshow, photos, screenshots with details of the Provider.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="multimedia" type="tns:multimediaPair"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="name" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Full Name of the (Multi-Provider Regional or Thematic) Catalogue.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="networks">
        <xs:annotation>
          <xs:documentation><![CDATA[Catalogues that are members of networks should list those networks here.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="network" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="participatingCountries">
        <xs:annotation>
          <xs:documentation><![CDATA[Catalogues that are funded/supported by several countries should list here all supporting countries (including the Coordinating country).]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="participatingCountry" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="publicContacts">
        <xs:annotation>
          <xs:documentation><![CDATA[List of the Catalogue's public contacts info.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="publicContact" type="tns:providerPublicContact"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="scientificDomains">
        <xs:annotation>
          <xs:documentation><![CDATA[A named group of providers that offer access to the same type of resource or capabilities.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="scientificDomain" type="tns:serviceProviderDomain"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="tags">
        <xs:annotation>
          <xs:documentation><![CDATA[Keywords associated to the Catalogue to simplify search by relevant keywords.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="tag" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="users">
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="tns:user"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="website" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Website with information about the (Multi-Provider Regional or Thematic) Catalogue.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="providerLocation">
    <xs:sequence>
      <xs:element name="city" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[City of incorporation or Physical location of the Provider or its coordinating centre in the case of distributed, virtual, and mobile providers.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="country" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Country of incorporation or Physical location of the Provider or its coordinating centre in the case of distributed, virtual, and mobile providers.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="postalCode" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Postal code of incorporation or Physical location of the Provider or its coordinating centre in the case of distributed, virtual, and mobile providers.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="region" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Region of incorporation or Physical location of the Provider or its coordinating centre in the case of distributed, virtual, and mobile providers.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="streetNameAndNumber" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Street and Number of incorporation or Physical location of the Provider or its coordinating centre in the case of distributed, virtual, and mobile providers.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="providerMainContact">
    <xs:sequence>
      <xs:element name="email" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Email of the Provider's main contact person/Provider manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="firstName" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[First Name of the Provider's main contact person/Provider manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="lastName" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Last Name of the Provider's main contact person/Provider manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="phone" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Phone of the Provider's main contact person/Provider manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="position" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Position of the Provider's main contact person/Provider manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="multimediaPair">
    <xs:sequence>
      <xs:element minOccurs="0" name="multimediaName" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Short description of the Multimedia content.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="multimediaURL" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Link to video, slideshow, photos, screenshots with details of the Provider.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="providerPublicContact">
    <xs:sequence>
      <xs:element name="email" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Email of the Provider's main contact person/Provider manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="firstName" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[First Name of the Provider's main contact person/Provider manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="lastName" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Last Name of the Provider's main contact person/Provider manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="phone" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Phone of the Provider's main contact person/Provider manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="position" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Position of the Provider's main contact person/Provider manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="serviceProviderDomain">
    <xs:sequence>
      <xs:element name="scientificDomain" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[The branch of science, scientific discipline that is related to the Resource.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="scientificSubdomain" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[The subbranch of science, scientific subdicipline that is related to the Resource.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="user">
    <xs:sequence>
      <xs:element minOccurs="0" name="email" type="xs:string"/>
      <xs:element minOccurs="0" name="id" type="xs:string"/>
      <xs:element minOccurs="0" name="name" type="xs:string"/>
      <xs:element minOccurs="0" name="surname" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="catalogueBundle">
    <xs:complexContent>
      <xs:extension base="tns:bundle">
        <xs:sequence>
          <xs:element minOccurs="0" ref="tns:catalogue"/>
          <xs:element minOccurs="0" name="status" type="xs:string"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="dataSource">
    <xs:complexContent>
      <xs:extension base="tns:service">
        <xs:sequence>
          <xs:element name="dataSourceClassification" type="xs:string">
            <xs:annotation>
              <xs:documentation><![CDATA[The specific type of the data source based on the vocabulary defined for this property]]></xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="jurisdiction" type="xs:string">
            <xs:annotation>
              <xs:documentation><![CDATA[The property defines the jurisdiction of the users of the data source, based on the vocabulary for this property]]></xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="persistentIdentitySystems">
            <xs:annotation>
              <xs:documentation><![CDATA[The persistent identifier systems that are used by the Data Source to identify the EntityType it supports]]></xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" name="persistentIdentitySystem" type="tns:persistentIdentitySystem"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="preservationPolicyURL" type="xs:anyURI">
            <xs:annotation>
              <xs:documentation><![CDATA[This policy provides a comprehensive framework for the long-term preservation of the research products.
Principles aims and responsibilities must be clarified. An important aspect is the description of preservation concepts to ensure the technical and conceptual
utility of the content]]></xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="researchEntityTypes">
            <xs:annotation>
              <xs:documentation><![CDATA[The types of OpenAIRE entities managed by the data source, based on the vocabulary for this property]]></xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" name="researchEntityType" type="xs:string"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="researchProductAccessPolicies">
            <xs:annotation>
              <xs:documentation><![CDATA[Research product access policy]]></xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" name="researchProductAccessPolicy" type="xs:string"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="researchProductLicensings">
            <xs:annotation>
              <xs:documentation><![CDATA[Licenses under which the research products contained within the data sources can be made available.
Repositories can allow a license to be defined for each research product, while for scientific databases the database is typically provided under a single license.]]></xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" name="researchProductLicensing" type="tns:researchProductLicensing"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="researchProductMetadataAccessPolicies">
            <xs:annotation>
              <xs:documentation><![CDATA[Research Product Metadata Access Policy]]></xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" name="researchProductMetadataAccessPolicy" type="xs:string"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="researchProductMetadataLicensing" type="tns:researchProductMetadataLicensing">
            <xs:annotation>
              <xs:documentation><![CDATA[Metadata Policy for information describing items in the repository:
Access and re-use of metadata]]></xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="submissionPolicyURL" type="xs:anyURI">
            <xs:annotation>
              <xs:documentation><![CDATA[This policy provides a comprehensive framework for the contribution of research products.
Criteria for submitting content to the repository as well as product preparation guidelines can be stated. Concepts for quality assurance may be provided.]]></xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="thematic" type="xs:boolean">
            <xs:annotation>
              <xs:documentation><![CDATA[Boolean value specifying if the data source is dedicated to a given discipline or is instead discipline agnostic]]></xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="versionControl" type="xs:boolean">
            <xs:annotation>
              <xs:documentation><![CDATA[If data versioning is supported: the data source explicitly allows the deposition of different versions of the same object]]></xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="service">
    <xs:sequence>
      <xs:element name="abbreviation" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[An abbreviation of the Resource Name as assigned by the Provider]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="accessModes">
        <xs:annotation>
          <xs:documentation><![CDATA[Eligibility/criteria for granting access to users (excellence-based, free-conditionally, free etc.).]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="accessMode" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="accessPolicy" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Information about the access policies that apply.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="accessTypes">
        <xs:annotation>
          <xs:documentation><![CDATA[The way a user can access the service/resource (Remote, Physical, Virtual, etc.).]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="accessType" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="catalogueId" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[The Catalogue this Resource is originally registered at.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="categories">
        <xs:annotation>
          <xs:documentation><![CDATA[A named group of Resources that offer access to the same type of Resources.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="category" type="tns:serviceCategory"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="certifications">
        <xs:annotation>
          <xs:documentation><![CDATA[List of certifications obtained for the Resource (including the certification body).]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
